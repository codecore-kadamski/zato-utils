# zato create server config
command=create_server
ca_certs_path=/opt/zato/ca/ca_cert.pem
cert_path=/opt/zato/ca/zato.server2.cert.pem
cluster_name={{CLUSTER_NAME}}
kvdb_host={{KVDB_HOST}}
kvdb_port={{KVDB_PORT}}
odb_db_name={{ODB_NAME}}
odb_password={{ODB_PASSWORD}}
